{
  "name": "Sprinkles",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Add custom scripts and styles to any webpage",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://sprinkles.app",
  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "matches": ["http://*/*", "https://*/*"],
      "js": ["script.js"]
    }
  ],
  "permissions": ["*://localhost/*"]
}
